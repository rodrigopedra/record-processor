{
  "name": "rodrigopedra/record-processor",
  "description": "Record Processor",
  "keywords": [
    "record processor",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Pedra Brum",
      "email": "rodrigo.pedra@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-pdo": "*",
    "psr/log": "^3.0",
    "illuminate/contracts": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/pipeline": "^11.0|^12.0",
    "league/csv": "^9.24",
    "phpoffice/phpspreadsheet": "^4.5"
  },
  "require-dev": {
    "symfony/console": "^7.3",
    "symfony/process": "^7.3",
    "symfony/var-dumper": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "RodrigoPedra\\RecordProcessor\\": "src/",
      "RodrigoPedra\\RecordProcessor\\Examples\\": "examples/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "suggest": {
    "ext-fileinfo": "to use file based readers/writers",
    "ext-dom": "to use HTML writer"
  }
}
